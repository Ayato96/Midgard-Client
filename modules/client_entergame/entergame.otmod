Module
  name: client_entergame
  description: Manages enter game and character list windows
  author: edubart
  website: https://github.com/edubart/otclient

  dependencies:
    - client_topmenu

  @onLoad: |
    dofile 'entergame'
    dofile 'characterlist'
    EnterGame.init()
    CharacterList.init()
    EnterGame.setUniqueServer("54.94.135.178", 7171, 772)

  @onUnload: |
    EnterGame.terminate()
    CharacterList.terminate()
